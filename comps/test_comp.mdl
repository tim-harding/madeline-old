# I'm a comment!

# Add option to fix gamma on loader? 
new load kitty
kitty.filename = "data/kitty.png"

new load tree
tree.filename = "data/tree.png"

new adjust linear_tree
linear_tree.gamma = 2.2
linear_tree.bg <- tree

new adjust linear_kitty
linear_kitty.gamma = 2.2
linear_kitty.bg <- kitty

new merge combine
combine.bg <- linear_tree
combine.fg <- linear_kitty
combine.tx = 200
combine.ty = 200

new shuffle weird
weird.bg <- combine
weird.r = 1
weird.g = 2
weird.b = 0
weird.a = 3

new blur gaussian
gaussian.bg <- weird
gaussian.size = 12

new adjust cc 
cc.bg <- gaussian
cc.saturation = 0.5
cc.exposure = 1.5
cc.gamma = 2.2
cc.invert_gamma = true

glob viewing = cc


